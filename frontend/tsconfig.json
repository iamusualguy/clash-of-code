{
	"compilerOptions": {
		"jsx": "react",
		"module": "commonjs",
		"noImplicitAny": true,
		"outDir": "./build/",
		"preserveConstEnums": true,
		"removeComments": true,
		"sourceMap": true,
		"target": "es5",
		"experimentalDecorators": true,
		"esModuleInterop": true
	},
	"include": [
		"**/*.tsx",
		"**/*.ts",
	],
	"exclude": [
		"node_modules"
	]
}